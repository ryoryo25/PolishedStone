{
	"multipart": [
		{
			"apply": {
				"model": "polishedstone_v2:lattice_oak_base"
			}
		},
		{
			"when": {
				"top": "true"
			},
			"apply": {
				"model": "polishedstone_v2:lattice_oak_top",
				"uvlock": true
			}
		},
		{
			"when": {
				"bottom": "true"
			},
			"apply": {
				"model": "polishedstone_v2:lattice_oak_top",
				"x": 180,
				"uvlock": true
			}
		},
		{
			"when": {
				"OR": [
					{
						"top": "true"
					},
					{
						"bottom": "true"
					}
				]
			},
			"apply": {
				"model": "polishedstone_v2:lattice_oak_middle",
				"uvlock": true
			}
		}
	]
}